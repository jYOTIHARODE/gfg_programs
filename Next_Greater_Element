#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,res=-1;  cin>>n;
    vector<int>arr(n);
    for(int i=0 ; i<n ; i++)    cin>>arr[i];

    for(int i=0 ; i<n ; i++)
    { 
        bool flag = false;
        for(int j=i+1;j<n;j++) 
        { 
            if(arr[i]<arr[j]) 
            { 
                flag =true;
                res=arr[j]; 
                break; 
            } 
        } 
        if(flag == true)
        cout<<res<<endl;
        else
        cout<<"-1"<<endl; 
    } 
    return 0;
}
