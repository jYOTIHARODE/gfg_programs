int klengthpref(string arr[], int n, int k, string str){    
        int count =0;
        for(int i=0 ; i<n ; i++)
        {
            int s =0 ;
            for(int j=0 ; j<k ; j++)
            {
                if(str[j]==arr[i][j])
                    s++;
            }
            if(s==k)    count++;
        }
        return count;
    }
