class Solution{
    public:
    
    int LastIndex(string s, char p){
        int n = s.size();
        int res = 0;
        for(int i=0 ; i<n ; i++)
        {
            if(p==s[i])
            {
                res = i;
            }
        }
        if(res!=0)  return res;
        else    return -1;
    }
};
